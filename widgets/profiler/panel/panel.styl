.b-cord-profiler-panel
  position: fixed
  z-index: 500
  left: 30%
  top: 0

  width: 70%
  max-height: 50%
  overflow-y: scroll

  background-color: white
  border: 1px solid black

  .e-expand-slowest
    font-size: 11px
    font-weight: bold
    line-height: 1.2
    padding: 1px 5px
    border-radius: 3px
    background-color: #5bc0de
    border: 1px solid #46b8da
    color: #fff
    vertical-align: middle
    margin: 5px
